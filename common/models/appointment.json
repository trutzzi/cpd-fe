{
    "name": "appointment",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "id": {
            "type": "number",
            "required": true
        },
        "start": {
            "type": "string",
            "required": true
        },
        "end": {
            "type": "string",
            "required": true
        },
        "description": {
            "type": "string"
        },
        "title": {
            "type": "string",
            "required": true
        },
        "userId": {
            "type": "string",
            "required": true
        },
        "confirmed": {
            "type": "boolean",
            "required": true,
            "default": true
        }
    },
    "validations": [],
    "relations": {
        "users": {
            "type": "belongsTo",
            "model": "User",
            "foreignKey": "userId",
            "primaryKey": "id",
            "options": {
                "nestRemoting": false,
                "disableInclude": false
            }
        }
    },
    "acls": [{
            "accessType": "WRITE",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY"
        },
        {
            "accessType": "WRITE",
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW",
            "property": "create"
        },
        {
            "accessType": "READ",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "ALLOW",
            "property": "*"
        },
        {
            "accessType": "WRITE",
            "principalType": "ROLE",
            "principalId": "$owner",
            "permission": "ALLOW",
            "property": "*"
        },
        {
            "accessType": "WRITE",
            "principalType": "ROLE",
            "principalId": "admin",
            "permission": "ALLOW",
            "property": "WRITE"
        }
    ],
    "methods": {}
}